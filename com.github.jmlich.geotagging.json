{
  "app-id": "com.github.jmlich.geotagging",
  "runtime": "org.kde.Platform",
  "runtime-version": "6.3",
  "base": "io.qt.qtwebengine.BaseApp",
  "base-version": "6.3",
  "sdk": "org.kde.Sdk",
  "command": "com.github.jmlich.geotagging",
  "finish-args": [
    "--share=ipc",
    "--socket=x11",
    "--socket=wayland",
    "--filesystem=home",
    "--share=network",
    "--device=dri"
  ],
  "modules": [
    {
      "name": "exiv2",
      "buildsystem": "cmake-ninja",
      "builddir": true,
      "config-opts": [
        "-DCMAKE_BUILD_TYPE=RelWithDebInfo"
      ],
      "sources": [
        {
          "type": "archive",
          "url": "https://exiv2.org/builds/exiv2-0.27.5-Source.tar.gz",
          "sha256": "35a58618ab236a901ca4928b0ad8b31007ebdc0386d904409d825024e45ea6e2"
        }
      ]
    },
    {
      "name": "geotagging",
      "buildsystem": "cmake-ninja",
      "config-opts": [
        "-DCMAKE_BUILD_TYPE=RelWithDebInfo",
        "-DVERSION:STRING=0.7.0"
      ],
      "sources": [
        {
          "type": "git",
          "url": "https://github.com/jmlich/geotagging.git",
          "tag": "0.7.0"
        }
      ]
    }
  ]
}
